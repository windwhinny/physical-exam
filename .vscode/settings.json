// 将设置放入此文件中以覆盖默认值和用户设置。
{
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/.js": true,
    "**/.js.map": true
  },
  "editor.tabSize": 2,
  "line-counter.excludes": [
    "**/.vscode/**",
    "**/node_modules/**",
    "**/*.js",
    "**/*.js.map"
  ]
}